      <param name="moduleName">Tour_view</param>
    </action>
    <action name="TourBalanceSubmit" class="OpTourBalanceAction" method="submit">
      <result>/includes/operator/TourBalanceDetail.jsp</result>
      <param name="moduleName">Tour_edit</param>
    </action>
    <action name="PrintTourBalance" class="OpTourBalanceAction" method="submitInput">
      <result>/includes/operator/PrintTourBalance.jsp</result>
      <param name="moduleName">Tour_view</param>
    </action>

    <!-- Print -->
    <action name="SimplePrint" class="com.opentravelsoft.action.manage.operator.SimpleReportAction" method="input">
      <result name="input">/includes/operator/report/SimplePrint.jsp</result>
      <param name="moduleName">Tour_view</param>
    </action>
    <action name="CustomerListPrint" class="com.opentravelsoft.action.manage.operator.CustomerListReportAction" method="input">
      <result name="input">/includes/operator/report/CustomerListPrint.jsp</result>
      <param name="moduleName">Tour_view</param>
    </action>
    <action name="THCustomerListPrint" class="com.opentravelsoft.action.manage.operator.THReportAction" method="input">
      <result name="input">/includes/operator/report/THCustomerListPrint.jsp</result>
      <param name="moduleName">Tour_view</param>
    </action>
    <action name="OutBandPrint" class="com.opentravelsoft.action.manage.operator.OutBandReportAction" method="input">
      <result name="input">/includes/operator/report/OutBandPrint.jsp</result>
      <param name="moduleName">Tour_view</param>
    </action>
    <action name="OutBandChange" class="com.opentravelsoft.action.manage.operator.OutBandReportAction" method="getObject">
      <result>/includes/operator/report/OutBandPrint.jsp</result>
      <param name="moduleName">Tour_view</param>
