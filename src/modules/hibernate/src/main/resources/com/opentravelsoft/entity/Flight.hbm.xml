<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.Flight" table="TBL_FLIGHT">
    <cache usage="read-write"/>
    <id name="recNo" type="string">
      <column name="REC_NO" length="10" />
      <generator class="assigned" />
    </id>
    <property name="fromCity1" type="string">
      <column name="FROM_CITY" length="3" />
    </property>
    <property name="toCity1" type="string">
      <column name="TO_CITY" length="3" />
    </property>
    <property name="flightNo" type="string">
      <column name="FLIGHT_NO" length="6" />
    </property>
    <property name="weekbit" type="string">
      <column name="WEEKBIT" length="7" />
    </property>
    <property name="fromTime" type="string">
      <column name="FROM_TIME" />
    </property>
    <property name="toTime" type="string">
      <column name="TO_TIME" />
    </property>
    <property name="nights" type="java.lang.Integer">
      <column name="NIGHTS" />
    </property>
    <property name="stopStation" type="java.lang.Integer">
      <column name="STOP_STATION" />
    </property>
    <property name="airModel" type="string">
      <column name="AIR_MODEL" length="12" />
    </property>
    <property name="airSeat" type="java.lang.Integer">
      <column name="AIR_SEAT" />
    </property>
    <property name="startDate" type="date">
      <column name="START_DATE" />
    </property>
    <property name="endDate" type="date">
      <column name="END_DATE" />
    </property>
    <property name="isDelete" type="java.lang.Integer">
      <column name="IS_DELETE" />
    </property>
    <many-to-one name="airways" class="com.opentravelsoft.entity.Airways" fetch="select" lazy="false">
      <column name="AIR_CODE"></column>
    </many-to-one>
    <many-to-one name="leavingAirport" class="com.opentravelsoft.entity.Airport" fetch="select" lazy="false" not-found="ignore">
      <column name="FROM_AIRPORT"></column>
    </many-to-one>
    <many-to-one name="goingAirport" class="com.opentravelsoft.entity.Airport" fetch="select" lazy="false" not-found="ignore">
      <column name="TO_AIRPORT"></column>
    </many-to-one>
  </class>
</hibernate-mapping>
