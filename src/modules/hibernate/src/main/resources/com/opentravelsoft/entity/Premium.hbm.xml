<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.Premium" table="TBL_CPIC_PREM">
    <id name="precode" type="string">
      <column name="PRECODE" length="8" />
      <generator class="assigned" />
    </id>
    <timestamp name="updated" column="UPDATED" />
    <property name="prem" type="java.lang.Double">
      <column name="PREM" precision="16" />
    </property>
    <property name="preday" type="java.lang.Integer">
      <column name="PREDAY" />
    </property>
    <property name="ywpre" type="java.lang.Double">
      <column name="YWPRE" precision="10" scale="3" />
    </property>
    <property name="ylpre" type="java.lang.Double">
      <column name="YLPRE" precision="10" scale="3" />
    </property>
    <property name="bcpre" type="java.lang.Double">
      <column name="BCPRE" precision="10" scale="3" />
    </property>
    <property name="clpre" type="java.lang.Double">
      <column name="CLPRE" precision="10" scale="3" />
    </property>
    <property name="hkpre" type="java.lang.Double">
      <column name="HKPRE" precision="10" scale="3" />
    </property>
    <property name="dodate" type="date">
      <column name="DODATE" />
    </property>
    <property name="note" type="string">
      <column name="NOTE" length="200" />
    </property>
    <property name="del" type="string">
      <column name="DEL" length="1" />
    </property>
    <property name="created" type="date" generated="insert">
      <column name="CREATED" default="current_date" />
    </property>
    <property name="createdBy" type="java.lang.Long">
      <column name="CREATEDBY" />
    </property>
    <property name="updatedBy" type="java.lang.Long">
      <column name="UPDATEDBY" />
    </property>
  </class>
</hibernate-mapping>
