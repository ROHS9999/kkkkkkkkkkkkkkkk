<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.TblPriceFile" table="TBL_PRICE_FILE">
    <id name="id" type="int">
      <column name="ID" />
      <generator class="native" />
    </id>
    <property name="groupId" type="int" not-null="true">
      <column name="GROUP_ID" />
    </property>
    <property name="filename" type="string">
      <column name="FILENAME" length="45" not-null="true" />
    </property>
    <property name="filepath" type="string">
      <column name="FILEPATH" length="45" not-null="true" />
    </property>
    <property name="filesize" type="java.lang.Integer">
      <column name="FILESIZE" />
    </property>
    <property name="created" type="date">
      <column name="CREATED" />
    </property>
    <property name="createdBy" type="java.lang.Long">
      <column name="CREATEDBY" />
    </property>
    <property name="overdue" type="date">
      <column name="OVERDUE" length="10" />
    </property>
    <property name="del" type="string">
      <column name="DEL" length="1" />
    </property>
    <property name="note" type="string">
      <column name="NOTE" length="30" />
    </property>
  </class>
</hibernate-mapping>
