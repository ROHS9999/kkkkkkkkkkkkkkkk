<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.opentravelsoft.entity.TblVisaItem" table="TBL_VISA_ITEM">
    <id name="visaitemId" type="int">
      <column name="VISAITEM_ID" />
      <generator class="native" />
    </id>
    <property name="recNo" type="string">
      <column name="REC_NO" length="10" not-null="true" />
    </property>
    <property name="itemId" type="java.lang.Integer">
      <column name="ITEM_ID" />
    </property>
    <property name="outline" type="string">
      <column name="OUTLINE" length="50" />
    </property>
    <property name="num" type="int">
      <column name="NUM" not-null="true" />
    </property>
    <property name="sortOrder" type="java.lang.Integer">
      <column name="SORT_ORDER" />
    </property>
    <property name="del" type="string">
      <column name="DEL" length="1" />
    </property>
  </class>
</hibernate-mapping>
